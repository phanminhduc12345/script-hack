-- Player
local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Main Player")
local InfiniteJumpEnabled;
local uis = game:GetService("UserInputService")

PlayerSection:NewToggle("Infinite Jump", "By toggling this you can jump infinitely.", function(State)
    InfiniteJumpEnabled = State
end)

local Player = game:GetService("Players").LocalPlayer
game:GetService("UserInputService").JumpRequest:Connect(function()
     if InfiniteJumpEnabled then
         Player.Character:WaitForChild("Humanoid"):ChangeState("Jumping")
     end
end)


PlayerSection:NewSlider("Walkspeed", "SPEED!!", 1000, 16, function(s)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

PlayerSection:NewSlider("Jumppower", "JUMP HIGH!!", 350, 50, function(s)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)


PlayerSection:NewButton("Chat Spoofer", "Lets you chat for other people", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/djBfk8Li'),true))()
end)



