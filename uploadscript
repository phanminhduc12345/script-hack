    loadstring(game:HttpGet("https://raw.githubusercontent.com/xOne2/One-x-HUB/main/README.md"))()
end)


BloxFruitSection:NewButton("FROPPA Hub", "FE Admin Commands", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/natoppo044/dkd/main/GG.lua"))()
end)

