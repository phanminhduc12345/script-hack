local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Synthetic script | Make By ! .#2841 | HN Gaming", "Midnight")

-- BloxFruit
local BloxFruit = Window:NewTab("Blox Fruit")
local BloxFruitSection = BloxFruit:NewSection("Script Blox Fruit")


